{
	"name": "OryKratos",
	"version": "0.13.0",
	"author": [
		"tesinormed"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:OryKratos",
	"descriptionmsg": "orykratos-desc",
	"license-name": "CC0-1.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43",
		"platform": {
			"php": ">= 8.1"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\OryKratos\\": "src/"
	},
	"config": {
		"OryKratosPublicHost": {
			"value": ""
		},
		"OryKratosAdminHost": {
			"value": ""
		},
		"OryKratosSessionCookie": {
			"value": "ory_kratos_session"
		},
		"OryKratosUiUrl": {
			"value": ""
		},
		"OryKratosLogoutUrl": {
			"value": ""
		}
	},
	"ConfigRegistry": {
		"orykratos": "GlobalVarConfig::newInstance"
	},
	"callback": "MediaWiki\\Extension\\OryKratos\\MainHooks::onRegistration",
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\OryKratos\\MainHooks",
			"services": [
				"MainConfig",
				"ConfigFactory"
			]
		},
		"user": {
			"class": "MediaWiki\\Extension\\OryKratos\\UserHooks",
			"services": [
				"ConnectionProvider",
				"OryKratos.Equivset",
				"OryKratos.IdentityApi"
			]
		},
		"session": {
			"class": "MediaWiki\\Extension\\OryKratos\\SessionHooks",
			"services": [
				"OryKratos.IdentityApi"
			]
		},
		"schema": {
			"class": "MediaWiki\\Extension\\OryKratos\\SchemaHooks"
		}
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "schema",
		"SkinTemplateNavigation::Universal": "main",
		"SpecialPage_initList": "main",
		"GetPreferences": "main",
		"LocalUserCreated": "user",
		"RenameUserComplete": "user",
		"UserLoadAfterLoadFromSession": "session"
	},
	"ServiceWiringFiles": [
		"src/ServiceWiring.php"
	],
	"SessionProviders": {
		"MediaWiki\\Extension\\OryKratos\\OryKratosSessionProvider": {
			"class": "MediaWiki\\Extension\\OryKratos\\OryKratosSessionProvider",
			"services": [
				"ConfigFactory",
				"OryKratos.FrontendApi",
				"OryKratos.IdentityApi",
				"UserIdentityLookup"
			]
		}
	},
	"RestRoutes": [
		{
			"path": "/orykratos/v0/username/usable",
			"method": "PUT",
			"class": "MediaWiki\\Extension\\OryKratos\\RestApiOryKratosUsernameUsable",
			"services": [
				"UserNameUtils",
				"UserIdentityLookup",
				"ConnectionProvider",
				"OryKratos.Equivset"
			]
		}
	],
	"MessagesDirs": {
		"OryKratos": [
			"i18n"
		]
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
