{
	"name": "OryKratos",
	"version": "0.1.1",
	"author": [
		"tesinormed"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:OryKratos",
	"descriptionmsg": "orykratos-desc",
	"license-name": "CC0-1.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43.0",
		"extensions": {
			"PluggableAuth": ">= 7.0.0"
		},
		"platform": {
			"php": ">= 8.1"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\OryKratos\\": "src/"
	},
	"callback": "MediaWiki\\Extension\\OryKratos\\OryKratos::onRegistration",
	"config": {
		"OryKratosHost": {
			"value": null
		}
	},
	"ConfigRegistry": {
		"orykratos": "GlobalVarConfig::newInstance"
	},
	"HookHandlers": {
		"schema": {
			"class": "MediaWiki\\Extension\\OryKratos\\SchemaHooks"
		}
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "schema"
	},
	"attributes": {
		"PluggableAuth": {
			"OryKratos": {
				"class": "MediaWiki\\Extension\\OryKratos\\OryKratos",
				"services": [
					"ConfigFactory",
					"AuthManager",
					"ConnectionProvider",
					"UserIdentityLookup"
				]
			}
		}
	},
	"MessagesDirs": {
		"OryKratos": [
			"i18n"
		]
	},
	"manifest_version": 2
}
